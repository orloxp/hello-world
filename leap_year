def is_leap_year(year=int):
    test = year / float(4)	
    century = year / float(100)
    centuryx4  = year / float(400)
    if test.is_integer():
        if century.is_integer():
            if centuryx4.is_integer():
            	return True
            else:
                return False
        else:
          return True
    else:
      return False

